@{
    Layout = "_WaiterLayout";
}
<div class="container">
    <div class="row">
        <div class="col-md-6">
            <span>Name: </span><br />
            <input type="text" id="txtUser" /><br />
            <span>Message:</span><br />
            <textarea rows="10" cols="50" id="txtMsg"></textarea><br />
            <button type="submit" class="btn btn-info" id="btnSend">Send</button>
        </div>
        <div class="col-md-6">
            <h3>Table Messages:</h3>
            <ul id="list" class="alert alert-info"></ul>
        </div>
    </div>
</div>

@section Scripts {
   <script src="~/lib/aspnet/signalr/dist/browser/signalr.js"></script>
   <script src="~/lib/aspnet/signalr/dist/browser/signalr.min.js"></script>
   <script src="~/js/Chat.js"></script>
    
}